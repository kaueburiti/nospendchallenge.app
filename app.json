{
  "expo": {
    "scheme": "nospendchallenge",
    "ios": {
      "associatedDomains": ["applinks:nospendchallenge.app"]
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "nospendchallenge.app",
              "pathPrefix": "/challenge"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
