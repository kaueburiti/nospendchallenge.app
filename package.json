    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "npx expo lint",
    "expo:doctor": "npx expo-doctor",
    "pod:install": "cd ios && pod install",
    "supabase:db-push": "supabase db push",
    "supabase:db-push-remote": "supabase db push --linked",
    "supabase:db-reset": "supabase db reset",
    "supabase:db-reset-remote": "supabase db reset --linked",
    "supabase:db-status": "supabase db status",
    "supabase:db-status-remote": "supabase db status --linked",
    "supabase:db-types": "npx supabase gen types --lang=typescript --project-id \"$PROJECT_REF\" --schema public > ./src/lib/db/database.types.ts",
    "supabase:local-db-types": "npx supabase gen types --lang=typescript --local > ./src/lib/db/database.types.ts",
    "supabase:seed": "npx tsx seed.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
